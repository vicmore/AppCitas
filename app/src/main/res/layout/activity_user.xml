<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserActivity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="76dp"
        android:layout_height="74dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:srcCompat="@mipmap/logo" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pepito Perez"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        tools:layout_editor_absoluteY="32dp" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="409dp"
        android:layout_height="653dp"
        android:layout_marginTop="26dp"
        app:layout_constraintTop_toBottomOf="@+id/textView25"
        tools:layout_editor_absoluteX="1dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/lblNombres"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Digite Nombres" />

            <EditText
                android:id="@+id/txtNombres"
                android:layout_width="399dp"
                android:layout_height="match_parent"
                android:contentDescription="Digite Nombres"
                tools:text="Digite Nombres" />


            <TextView
                android:id="@+id/lblIdentificacion"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Dibite Identificación" />

            <EditText
                android:id="@+id/txtIdentificacion"
                android:layout_width="399dp"
                android:layout_height="match_parent"
                android:contentDescription="Digite Identificación"
                tools:text="Digite Identificación" />

            <TextView
                android:id="@+id/lblDireccion"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Digite Dirección" />

            <EditText
                android:id="@+id/txtDireccion"
                android:layout_width="399dp"
                android:layout_height="match_parent"
                android:contentDescription="Digite Dirección"
                tools:text="Digite Dirección" />

            <TextView
                android:id="@+id/lblCiudad"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Digite Ciudad" />

            <EditText
                android:id="@+id/txtCiudad"
                android:layout_width="399dp"
                android:layout_height="match_parent"
                android:contentDescription="Digite Ciudad"
                tools:text="Digite Ciudad" />

            <TextView
                android:id="@+id/lblCelular"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Digite Celular" />

            <EditText
                android:id="@+id/txtCelular"
                android:layout_width="399dp"
                android:layout_height="match_parent"
                android:contentDescription="Digite Celular"
                tools:text="Digite Celular" />

            <TextView
                android:id="@+id/lblFijo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Digite Fijo" />

            <EditText
                android:id="@+id/txtFijo"
                android:layout_width="399dp"
                android:layout_height="match_parent"
                android:contentDescription="Digite Fijo"
                tools:text="Digite Fijo" />

            <TextView
                android:id="@+id/lblEmail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Digite Email" />

            <EditText
                android:id="@+id/txtEmail"
                android:layout_width="399dp"
                android:layout_height="match_parent"
                android:contentDescription="Digite Email"
                tools:text="Digite Email" />

            <TextView
                android:id="@+id/lblContacto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Digite Contacto" />

            <EditText
                android:id="@+id/txtContacto"
                android:layout_width="399dp"
                android:layout_height="match_parent"
                android:contentDescription="Digite Contacto"
                tools:text="Digite Contacto" />

            <TextView
                android:id="@+id/lblCelContacto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Digite Celular Contacto" />

            <EditText
                android:id="@+id/txtCelContacto"
                android:layout_width="399dp"
                android:layout_height="match_parent"
                android:contentDescription="Digite Cel Contacto"
                tools:text="Digite Cel Contacto" />

            <TextView
                android:id="@+id/lblLogin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Digite Login" />

            <EditText
                android:id="@+id/txtLogin"
                android:layout_width="399dp"
                android:layout_height="match_parent"
                android:contentDescription="Digite Login"
                tools:text="Digite Login" />

            <TextView
                android:id="@+id/lblPassword"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Digite Password" />

            <EditText
                android:id="@+id/editTextTextPassword"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPassword" />


            <Button
                android:id="@+id/btnActualzar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Actualizar"
                android:width="50dp" />

            <Button
                android:id="@+id/btnBackSearch"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Regresar"
                android:width="50dp" />
        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>